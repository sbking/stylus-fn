  list-string: test(list-string)
  list-hash: test(list-hash)
  pairs-unit: test(pairs-unit)
  pairs-ident: test(pairs-ident)
  pairs-string: test(pairs-string)
  hash-ident: test(hash-ident)
  hash-string: test(hash-string)