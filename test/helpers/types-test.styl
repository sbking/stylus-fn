unit = 42px
ident = foo
string = "foo"
list-unit = 1 2 3 4
list-ident = foo bar baz
list-string = "foo" "bar" "baz"
list-hash = {
  foo: "bar"
}, {
  baz: "qux"
}
pairs-unit = (0 1) (1 2) (2 3) (3 4)
pairs-ident = (foo "bar") (baz "qux")
pairs-string = ("foo" "bar") ("baz" "qux")
